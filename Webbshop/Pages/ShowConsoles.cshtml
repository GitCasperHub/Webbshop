@page
@model Webbshop.Pages.ShowConsolesModel
@{
}
<h3 class="text-center">Here is a list of different consoles we're selling'</h3>



@foreach (var gameConsole in Model.Consoles)
            {
<div class="card mx-auto row" style="width: 18rem;">
    <img class="card-img-top img-thumbnail " src="@gameConsole.ConsoleImageURL" alt="Card image cap">
    <div class="card-body">
        <h4 class="card-title ">@gameConsole.ConsoleName</h4>
        <p>@gameConsole.ConsoleDescription</p>
        <h5> Pris: @gameConsole.ConsolePrice Kr</h5>
        <div></div>
    </div>
</div>
            }